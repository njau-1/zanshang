<script type="text/javascript">
    $(".q_box p").click(function () {
        $(this).find("span").addClass("checkout");
        $(this).siblings().find("span").removeClass();
    })


    $("#a_btn").click(function () {
        // 答案逻辑
        var answer = $(".checkout"),
                a = [];
        b = '';
        if (answer.length < 10) {
            alert("请检查是否漏选答案！")
        } else {
            for (i = 0; i < 10; i++) {
                a.push($(answer[i]).html());
                b = b + $(answer[i]).html().substring(0, 1);
            }
            window.location.href = "/authentication?return=%2Fmarketing%2Fh5%2Fheywq%2Fanswer%3Fanswer%3D" + b;
        }
    })


</script>
